{
	"name": "Zuquini Theme",
	"type": "dark",
	"colors": {
		// Editor
		"editor.background": "#1c1c1c",
		"editor.foreground": "#f2f2f2",
		"editor.selectionBackground": "#88ccff28",
		"editor.lineHighlightBorder": "#292929",
		"editor.lineHighlightBackground": "#00000010",
		"editor.findMatchBackground": "#974316",
		"editorBracketMatch.border": "#ffff009c",
		"editorError.foreground": "#ff3333",
		"editorHoverWidget.background": "#232323",
		"editorIndentGuide.background": "#2a2a2a",
		"editorIndentGuide.activeBackground": "#505050",
		"editorSuggestWidget.background": "#232323",
		"editorUnnecessaryCode.opacity": "#00000088",
		"editorWhitespace.foreground": "#373737",
		"editorWidget.resizeBorder": "#ff4400",
		// Side Menu
		"activityBar.background": "#282828",
		"activityBar.activeBorder": "#ff4400",
		"activityBar.inactiveForeground": "#666666",
		"sideBar.background": "#202020",
		"sideBar.foreground": "#cccccc",
		"sideBarTitle.foreground": "#d4d4d4",
		"sideBarSectionHeader.background": "#282828",
		"sideBarSectionHeader.foreground": "#cccccc",
		"list.inactiveSelectionBackground": "#222b35",
		"list.hoverBackground": "#242f3c",
		// Scrollbar
		"scrollbarSlider.background": "#fff2",
		"scrollbarSlider.hoverBackground": "#ffffff29",
		"scrollbarSlider.activeBackground": "#ffffff21",
		// Title bar - Settings: "window.titleBarStyle": "custom"
		"titleBar.activeBackground": "#282828",
		"titleBar.inactiveBackground": "#282828",
		"titleBar.border": "#282828",
		"titleBar.activeForeground": "#cccccc",
		"titleBar.inactiveForeground": "#aaaaaa",
		// Tabs
		"tab.activeBackground": "#1c1c1c",
		"tab.inactiveBackground": "#282828",
		"tab.activeBorder": "#1c1c1c",
		"tab.border": "#282828",
		"tab.activeBorderTop": "#ff4400",
		"editorGroupHeader.tabsBackground": "#282828",
		// Tabs
		"statusBarItem.remoteBackground": "#009060",
		"statusBarItem.remoteForeground": "#fff",
		// Others
		"breadcrumbPicker.background": "#232323",
		"debugTokenExpression.error": "#ff6969",
		"debugTokenExpression.string": "#ff9b65",
		"terminal.selectionBackground": "#88ccff28",
		// Extensions
		"bookmarks.lineBackground": "#007aff0a",
		"gitlens.trailingLineForegroundColor": "#ffffff15"
	},
	"tokenColors": [
		{
			"scope": [
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6A9955",
				"fontStyle": "italic",
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#b8ce99"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
